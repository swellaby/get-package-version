{
  "files.exclude": {
    "**/*.js": { "when": "$(basename).ts" },
    "**/*d.ts": { "when": "$(basename).ts" },
    "**/*.map": { "when": "$(basename).map" },
    "npm-debug.log": true,
    ".coverage": true,
    ".testresults": true,
    ".nyc_output": true,
    "**/node_modules": true,
    "**/*.tgz*": true,
    "**/*.tar.*": true
  },
  "files.associations": {
    "**/.azure-pipelines/**": "azure-pipelines"
  }
}
